<h2>Edit City</h2>

<%= form_for @city do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

   <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit %>
<% end %>

<%= form_for @city, method: :delete do |f| %>
  <%= f.submit "Delete!", class: "delete" %>
<% end %>