<h2>&nbsp;</h2>
<%= form_for [@city, @post] do |f| %>
<legend>New Post</legend>
  <fieldset>
    <div class="field-set">
      <%= f.label :title%>
      <%= f.text_field :title %>
      
      <%= f.label :text %>
      <%= f.text_area :text, size: '50x10' %>

      <%= f.submit class: "button" %>
    </div>
  </fieldset>
<% end %>