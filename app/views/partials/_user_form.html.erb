

<%= form_for @users do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :image %>
  <%= f.text_field :image %>
  
  <%= f.label :location %>
  <%= f.text_field :location %>
  
  <%= f.submit %>
<% end %>

